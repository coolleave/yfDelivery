
.commonly-hover{
	background-color: #eee;
}
.scroll-list{
	flex: 1;
	height: 100vh;
	overflow-y: hidden;
}
.phone-list{
	display: flex;
	background-color: #fff;
	flex-direction:column;
	position:relative;
	width: 100%;
}
.list-item {
	width: 100%;
	display: flex;
	align-items: center;
	flex-wrap:wrap;
	height: 92rpx;
	background-color: #fff;
	height: 100%;
}
.list-item >.list-item-phone{
	font-weight: normal;
}
.list-item-title{
	background-color: #eee;
}
.list-item-title,.list-item-phone{
	width: 100%;
	height: 92rpx;
	line-height: 92rpx;
	font-size: 32rpx;
	font-weight: bold;
	padding: 0 20rpx;
	border-bottom: 1px solid #e5e5e5;
}




